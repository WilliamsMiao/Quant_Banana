# 敏感信息配置文件模板
# 复制此文件为 secrets.yaml 并填入真实的敏感信息
# ⚠️ 请确保 secrets.yaml 不被提交到Git

# Futu OpenD WebSocket密钥
futu:
  ws_key: "your_futu_ws_key_here"

# 钉钉机器人配置
dingding:
  webhook: "https://oapi.dingtalk.com/robot/send?access_token=YOUR_ACCESS_TOKEN"
  secret: "YOUR_DINGDING_SECRET"

dingding_tuning:
  webhook: "https://oapi.dingtalk.com/robot/send?access_token=YOUR_ACCESS_TOKEN"
  secret: "YOUR_DINGDING_SECRET"

# 数据库密码（如需要）
database:
  password: "your_database_password"

# iTick API配置
itick:
  token: "your_itick_token_here"

# 新浪云API配置（如使用SAE API）
sina:
  access_key: "your_sina_access_key"
  secret_key: "your_sina_secret_key"

